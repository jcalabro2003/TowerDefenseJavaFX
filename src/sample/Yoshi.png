<script>
function getCookie(cname) {
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i = 0; i < ca.length; i++) {
var c = ca[i];
while (c.charAt(0) == ' ') {
c = c.substring(1);
}
if (c.indexOf(name) == 0) {
return c.substring(name.length, c.length);
}
}
return "";
}
function setCookie(cname, cvalue, exdays) {
var d = new Date();
d.setTime(d.getTime() + (exdays*24*60*60*1000));
var expires = "expires="+d.toUTCString();
document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}
if(getCookie("checkCookie")!="1"){
document.getElementById("bandeauCookie").style.display="block";
}
function closeCookie(){
setCookie("checkCookie","1",365);
if(document.body.clientWidth>420){
document.getElementById("bandeauCookie").style.right="100px";
setTimeout(function(){document.getElementById("bandeauCookie").style.right="-500px";}, 100);
}else{
document.getElementById("bandeauCookie").style.bottom="-500px";
}
}
</script>
